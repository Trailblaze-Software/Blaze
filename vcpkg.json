{
  "name": "blaze",
  "version": "0.0.0",
  "dependencies": [
    {
      "name": "gdal",
      "version>=": "3.9.0"
    },
    {
      "name": "opencv4",
      "version>=": "4.10.0"
    }
  ],
  "features": {
    "pdal": {
      "description": "Support for PDAL",
      "dependencies": [
        "pdal"
      ]
    },
    "gui": {
      "description": "Support for GUI",
      "dependencies": [
        {
          "name": "qtbase",
          "features": [
            "gui",
            "widgets",
            "concurrent",
            "opengl"
          ]
        },
        "qtimageformats",
        "qtsvg"
      ]
    }
  }
}
