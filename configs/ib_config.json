          {
            "blocking_threshold": 0.1,
            "color": "white"
          }
        ]
      },
      {
        "name": "green",
        "min_height": 0.5,
        "max_height": 2.5,
        "colors": [
          {
            // slow
            "blocking_threshold": 0.2,
            "color": "30% green"
          },
          {
            // walk
            "blocking_threshold": 0.6,
            "color": "60% green"
          },
          {
            // fight
            "blocking_threshold": 0.8,
            "color": "green"
          }
        ]
      }
    ]
  },
  "buildings": {
    "color": "black"
  },
  "render": {
    "scale": 25000,
    "dpi": 600
