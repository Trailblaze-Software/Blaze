  "contours": {
    "form": {
      "interval": 2.5,
      "min_points": 5,
      "color": "brown",
      "width": 0.08
    },
    "normal": {
      "interval": 5.0,
      "min_points": 5,
      "color": "brown",
      "width": 0.14
    },
    "index": {
      "interval": 25.0,
      "min_points": 5,
      "color": "brown",
      "width": 0.25
    }
  },
  "water": {
    "minor": {
      "color": "blue",
      "catchment": 0.03,
      "width": 0.18
    },
    "major": {
      "color": "blue",
      "catchment": 0.2,
      "width": 0.3
